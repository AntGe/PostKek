@model IEnumerable<PostKek.Models.ViewModels.SingleCommenVm>  
@foreach (var item in Model)
{
    <div class="particular-comment">
        <div class="profile"> 
            <h2>@item.User.Name</h2>
        </div>
        <div class="comment-self">
            <span>
                <p>@item.Contents</p> 
            </span>
        </div>
    </div>
} 
